{
    "name": "mcfog/nimo",
    "description": "Nimo Is your Middleware Organizer",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "middleware",
        "psr",
        "psr-7",
        "http",
        "http-message"
    ],
    "authors": [
        {
            "name": "McFog Wang",
            "email": "mcfogw@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Nimo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nimo\\Tests\\": "tests/"
        }
    },
    "require": {
        "http-interop/http-server-handler": "^1.0",
        "http-interop/http-server-middleware": "^1.0",
        "php": ">=7.1.0",
        "psr/http-message": "^1.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.3.x-dev"
        }
    },
    "suggest": {
        "zendframework/zend-diactoros": "PSR-7 implementation and middleware runner"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    }
}
